    <participant profile_name="usv_unicast_profile" is_default_profile="true">
        <rtps>
            <builtin>
                <discovery_config>
                    <discoveryProtocol>SIMPLE</discoveryProtocol>
                </discovery_config>
                <initialPeersList>
                    <!-- 指向 地面站 (GS) 的 IP -->
                    <locator>
                        <udpv4>
                            <address>192.168.68.53</address>
                        </udpv4>
                    </locator>
                </initialPeersList>
            </builtin>
        </rtps>
    </participant>
</profiles>
